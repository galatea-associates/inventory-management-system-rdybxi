    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "react-scripts": "5.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-explorer": "^2.5.3",
    "start-server-and-test": "^2.0.0",
