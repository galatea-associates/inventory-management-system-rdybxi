REACT_APP_API_BASE_URL=https://api-staging.ims.example.com
REACT_APP_WEBSOCKET_URL=wss://ws-staging.ims.example.com
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Environment Configuration
REACT_APP_ENVIRONMENT=staging
REACT_APP_ENABLE_MOCK_API=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_LOG_LEVEL=info

# Authentication and Session Management
REACT_APP_AUTH_ENABLED=true
REACT_APP_SESSION_TIMEOUT=1200000
REACT_APP_REFRESH_INTERVAL=30000

# Upload Settings
REACT_APP_MAX_UPLOAD_SIZE=5242880

# Feature Flags
REACT_APP_FEATURE_LOCATE_MANAGEMENT=true
REACT_APP_FEATURE_SHORT_SELL_APPROVAL=true
REACT_APP_FEATURE_CALCULATION_RULES=true
REACT_APP_FEATURE_ANALYTICS=true
REACT_APP_FEATURE_EXCEPTION_MANAGEMENT=true

# Performance and Monitoring
REACT_APP_PERFORMANCE_MONITORING=true
REACT_APP_CORS_ENABLED=false

# Development and Debugging Settings
REACT_APP_DEBUG_TOOLS=true
REACT_APP_SHOW_VERBOSE_ERRORS=true
REACT_APP_GENERATE_SOURCEMAP=true
REACT_APP_BUILD_VERSION=staging-1.0.0
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_DISABLE_CACHE=false
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_DEVTOOLS=true
REACT_APP_ENABLE_LOGGER=true

# Security Settings
REACT_APP_CONTENT_SECURITY_POLICY=default-src 'self'; connect-src 'self' https://api-staging.ims.example.com wss://ws-staging.ims.example.com; img-src 'self' data:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval';
REACT_APP_STRICT_TRANSPORT_SECURITY=max-age=31536000; includeSubDomains